{
  "name": "@windijs/utilities",
  "version": "1.0.2",
  "description": "@windijs/utilities",
  "main": "dist/utilities.js",
  "module": "dist/utilities.mjs",
  "types": "dist/utilities.d.ts",
  "exports": {
    ".": {
      "import": "./dist/utilities.mjs",
      "require": "./dist/utilities.js",
      "types": "./dist/utilities.d.ts"
