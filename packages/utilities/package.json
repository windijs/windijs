{
  "name": "@windijs/utilities",
  "version": "1.0.12",
  "description": "@windijs/utilities",
  "main": "dist/windijsUtilities.js",
  "module": "dist/es/index.js",
  "types": "dist/windijsUtilities.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/windijsUtilities.js",
      "types": "./dist/windijsUtilities.d.ts"
    },
    "./proxy": {
      "import": "./dist/proxy/es/index.js",
      "require": "./dist/proxy/windijsUtilities.js",
      "types": "./dist/proxy/windijsUtilities.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "windi",
    "windijs",
    "windicss"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "@windijs/utilities"
  },
  "buildOptions": {
    "name": "windijsUtilities",
    "formats": [
      "cjs",
      "mjs",
      "runtime"
    ]
  },
  "author": "Raven Satir",
  "license": "MIT",
  "dependencies": {
    "@windijs/core": "1.1.6",
    "@windijs/colors": "1.0.4",
    "@windijs/config": "1.0.10",
    "@windijs/helpers": "1.1.5"
  }
}
