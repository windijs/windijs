    }
  },
  "files": [
    "dist",
    "gen"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "windi",
    "windijs",
    "windicss"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "@windijs/utilities"
  },
  "author": "Raven Satir",
  "license": "MIT",
  "dependencies": {
    "@windijs/core": "1.0.2",
    "@windijs/colors": "1.0.1",
    "@windijs/config": "1.0.2",
    "@windijs/helpers": "1.0.1"
  }
}
