{
  "name": "@windijs/style",
  "version": "1.1.5",
  "description": "@windijs/style",
  "main": "dist/windijsStyle.js",
  "module": "dist/windijsStyle.mjs",
  "types": "dist/windijsStyle.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "windi",
    "windijs",
    "windicss"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/windijsStyle.mjs",
      "require": "./dist/windijsStyle.js"
    }
  },
  "dependencies": {
    "@windijs/core": "1.1.5",
    "@windijs/helpers": "1.1.5",
    "@windijs/shared": "1.0.6"
  },
  "buildOptions": {
    "name": "windijsStyle",
    "formats": [
      "cjs",
      "mjs",
      "runtime"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "@windijs/style",
    "readmeFile": "./README.md"
  },
  "author": "Raven Satir",
  "license": "MIT"
}
