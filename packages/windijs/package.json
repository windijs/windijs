{
  "name": "windijs",
  "version": "1.1.8",
  "description": "utility-first css-in-js framework",
  "main": "dist/windijs.js",
  "module": "dist/windijs.mjs",
  "types": "dist/windijs.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "windi",
    "windijs",
    "windicss"
  ],
  "author": "Raven Satir",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/windijs.mjs",
      "require": "./dist/windijs.js"
    }
  },
  "buildOptions": {
    "name": "windijs",
    "formats": [
      "cjs",
      "mjs",
      "runtime"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "windijs",
    "readmeFile": "./README.md"
  },
  "dependencies": {
    "@windijs/style": "1.1.8",
    "@windijs/colors": "1.0.4",
    "@windijs/config": "1.0.10",
    "@windijs/core": "1.1.8",
    "@windijs/helpers": "1.1.6",
    "@windijs/shared": "1.0.7"
  }
}
