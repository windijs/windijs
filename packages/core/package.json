{
  "name": "@windijs/core",
  "version": "1.1.4",
  "description": "@windijs/core",
  "main": "dist/windijsCore.js",
  "module": "dist/windijsCore.mjs",
  "types": "dist/windijsCore.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "windi",
    "windijs",
    "windicss"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/windijsCore.mjs",
      "require": "./dist/windijsCore.js"
    }
  },
  "author": "Raven Satir",
  "license": "MIT",
  "dependencies": {
    "@windijs/shared": "1.0.5",
    "@windijs/helpers": "1.1.4"
  },
  "buildOptions": {
    "name": "windijsCore",
    "formats": [
      "cjs",
      "mjs",
      "runtime"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "@windijs/core"
  }
}
